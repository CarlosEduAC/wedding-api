{
  "name": "wedding-api",
  "version": "1.0.0",
  "description": "Tribe Cross Finance Node Template",
  "main": "index.js",
  "keywords": [
    "node",
    "typescript",
    "express",
    "jest",
    "eslint",
    "prettier"
  ],
  "author": "CarlosEduAC",
  "license": "MIT",
  "scripts": {
    "build": "npm run clean && swc ./src -d ./dist --config-file .swcrc",
    "start": "node ./dist/src/server.js",
    "start:dev": "export NODE_TLS_REJECT_UNAUTHORIZED=0 && NODE_ENV=development tsx watch src/server",
    "test": "npm run clean:tests && NODE_ENV=test jest --collectCoverage ./tests",
    "lint": "eslint src/ tests/",
    "lint:fix": "eslint src/ tests/ --fix",
    "format": "prettier --ignore-path .gitignore --check \"**/*.+(ts|json)\"",
    "format:fix": "prettier --ignore-path .gitignore --write \"**/*.+(ts|json)\"",
    "clean": "rm -rf dist",
    "clean:tests": "rm -rf coverage reports",
    "swagger:generate": "tsx src/swagger.ts"
  },
  "dependencies": {
    "@elastic/ecs-pino-format": "^1.5.0",
    "@prisma/client": "^5.21.1",
    "axios": "^1.7.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.1",
    "http-status-codes": "^2.3.0",
    "pino": "^9.3.2",
    "uuid": "^11.0.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/js": "^9.8.0",
    "@jest/globals": "^29.7.0",
    "@swc/cli": "^0.5.2",
    "@swc/core": "^1.7.5",
    "@swc/jest": "^0.2.36",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.17.0",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.8.0",
    "globals": "^15.9.0",
    "husky": "^9.1.4",
    "jest": "^29.7.0",
    "jest-sonar-reporter": "^2.0.0",
    "prettier": "^3.3.3",
    "prisma": "^6.14.0",
    "sonarqube-scanner": "^4.0.1",
    "supertest": "^7.0.0",
    "swagger-autogen": "^2.23.7",
    "swagger-ui-express": "^5.0.1",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.16.5",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.0.0"
  },
  "jestSonar": {
    "reportPath": "reports",
    "reportFile": "test-report.xml",
    "indent": 4
  }
}
